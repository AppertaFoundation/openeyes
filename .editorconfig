# editorconfig.org

# Top-most EditorConfig file
root = true

# Global
[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = false

[*.{php,js,css,html,pp,scss,json,sh,less}]
indent_style = space
indent_size = 4

[node_modules/**.js]
indent_style = space
indent_size = 2
