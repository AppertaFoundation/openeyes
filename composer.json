{
  "name": "openeyes/openeyes",
  "description": "Electronic Patient Record system for use in Ophthalmology clinics",
  "require": {
    "yiisoft/yii": "1.1.21",
    "rlanvin/php-rrule": "1.2.0",
    "endroid/qr-code": "^1.7",
    "setasign/fpdi-fpdf": "1.6.1",
    "xthiago/pdf-version-converter": "dev-master"
  },
  "require-dev": {
    "behat/behat": "2.*@stable",
    "behat/mink": "1.7.*@stable",
    "behat/yii-extension": "1.*@stable",
    "behat/mink-extension": "1.3.*@stable",
    "behat/mink-goutte-driver": "1.2.*@stable",
    "behat/mink-selenium2-driver": "1.3.*@stable",
    "fabpot/goutte": "1.0.*@stable",
    "sensiolabs/behat-page-object-extension": "1.*@stable",
    "phpunit/phpunit": "3.7.*",
    "phpunit/phpunit-selenium": "1.3.*",
    "phpunit/dbunit": "1.2.*",
    "phpunit/php-invoker": "1.1.*",
    "phpunit/phpunit-story": "*",
    "phploc/phploc": "*",
    "pdepend/pdepend": "2.2.4",
    "phpmd/phpmd": "@stable",
    "sebastian/phpcpd": "*",
    "squizlabs/php_codesniffer": "3.*"
  },
  "config": {
    "bin-dir": "bin"
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.org"
    }
  },
  "autoload": {
    "classmap": ["features/bootstrap/Pages/OpenEyesPage.php"]
  }
}
